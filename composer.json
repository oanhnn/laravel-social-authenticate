{
  "name": "oanhnn/laravel-social-authenticate",
  "type": "library",
  "description": "Easy integrate Laravel Socialite into Laravel 5.5+ Application",
  "keywords": [
    "laravel",
    "package",
    "socialite",
    "social-login",
    "login",
    "oauth",
    "social",
    "authenticate"
  ],
  "license": "MIT",
  "homepage": "https://github.com/oanhnn/laravel-social-authenticate",
  "support": {
    "issues": "https://github.com/oanhnn/laravel-social-authenticate/issues",
    "source": "https://github.com/oanhnn/laravel-social-authenticate"
  },
  "authors": [
    {
      "name": "Oanh Nguyen",
      "email": "oanhnn.bk@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Laravel\\SocialAuthenticate\\": "src/"
    }
  },
  "require": {
    "php": "^7.1.3",
    "illuminate/contracts": "^6.0|^7.0|^8.0",
    "illuminate/database": "^6.0|^7.0|^8.0",
    "illuminate/support": "^6.0|^7.0|^8.0",
    "laravel/socialite": "^4.0"
  },
  "require-dev": {
    "guzzlehttp/guzzle": "^6.3|^7.0",
    "orchestra/testbench": "^4.0|^5.0|^6.0",
    "phpunit/phpunit": "^8.0|^9.0"
  },
  "scripts": {
    "test": [
      "phpunit --testdox --testsuite=Tests"
    ],
    "coverage": [
      "phpunit --coverage-html storage/coverage --testsuite=Tests"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Laravel\\SocialAuthenticate\\ServiceProvider"
      ]
    }
  }
}
